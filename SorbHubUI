TeleportSection:AddButton({
    Title = "Teleport to Fishing Area",
    Content = "Teleport to fishing area",
    Icon = "rbxassetid://10734949856",
    Callback = function()
        print("[SorbHub] Teleporting to fishing area...")
    end
})

TeleportSection:AddButton({
    Title = "Teleport to Shop",
    Content = "Teleport to shop location",
    Icon = "rbxassetid://10734949856",
    Callback = function()
        print("[SorbHub] Teleporting to shop...")
    end
})

-- MISC TAB
local MiscSection = MiscTab:AddSection("Miscellaneous", true)

MiscSection:AddToggle({
    Title = "Anti AFK",
    Content = "Prevent getting kicked for being AFK",
    Default = true,
    Callback = function(value)
        print("[SorbHub] Anti AFK:", value)
        if value then
            local VirtualUser = game:GetService("VirtualUser")
            game.Players.LocalPlayer.Idled:Connect(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
            end)
        end
    end
})

MiscSection:AddButton({
    Title = "Rejoin Server",
    Content = "Rejoin the current server",
    Icon = "rbxassetid://10734949856",
    Callback = function()
        print("[SorbHub] Rejoining server...")
        game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
    end
})

local SettingsSection = MiscTab:AddSection("Settings", false)

SettingsSection:AddSlider({
    Title = "UI Scale",
    Content = "Adjust UI size",
    Increment = 1,
    Min = 80,
    Max = 120,
    Default = 100,
    Callback = function(value)
        print("[SorbHub] UI Scale:", value)
    end
})

SettingsSection:AddInput({
    Title = "Custom Value",
    Content = "Enter a custom value",
    Default = "",
    Callback = function(value)
        print("[SorbHub] Custom Value:", value)
    end
})

-- Success Notification
Window:SetNotification({
    Title = "SorbHub",
    Description = "Loaded!",
    Content = "SorbHub has been loaded successfully. Version 1.0.0",
    Time = 0.5,
    Delay = 3
})

print("=================================")
print("SorbHub UI loaded successfully!")
print("Version: 1.0.0")
print("Manual UI - No Library Required")
print("=================================")

-- Anti-AFK Auto Enabled
local VirtualUser = game:GetService("VirtualUser")
game.Players.LocalPlayer.Idled:Connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)
